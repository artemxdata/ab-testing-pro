  "keywords": [
    "ab-testing",
    "statistics",
    "bayesian",
    "machine-learning",
    "data-science",
    "analytics",
    "conversion-optimization",
    "react",
    "typescript",
    "tailwind",
    "pwa",
    "roi-analysis",
    "statistical-analysis"
  ],
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com",
