    "utilities",
    "developer"
  ],
  "screenshots": [
    {
      "src": "screenshot-desktop.png",
      "sizes": "1280x720",
      "type": "image/png",
      "platform": "wide",
      "label": "A/B Testing Pro Dashboard on Desktop"
    },
    {
      "src": "screenshot-mobile.png", 
      "sizes": "375x812",
      "type": "image/png",
      "platform": "narrow",
      "label": "A/B Testing Pro on Mobile"
    }
  ],
  "shortcuts": [
    {
      "name": "New A/B Test",
      "short_name": "New Test",
      "description": "Create a new A/B test analysis",
      "url": "/?tab=planning",
      "icons": [
        {
          "src": "shortcut-new-test.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Analytics Dashboard", 
      "short_name": "Dashboard",
      "description": "View real-time analytics dashboard",
      "url": "/?tab=dashboard",
      "icons": [
        {
          "src": "shortcut-dashboard.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "ML Predictions",
      "short_name": "ML Insights", 
      "description": "AI-powered test predictions",
      "url": "/?tab=ml",
      "icons": [
        {
          "src": "shortcut-ml.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "protocol_handlers": [
    {
      "protocol": "web+abtest",
      "url": "/analyze?data=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/import",
      "accept": {
        "text/csv": [".csv"],
        "application/json": [".json"],
        "application/vnd.ms-excel": [".xlsx", ".xls"]
      }
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "data_file",
          "accept": ["text/csv", "application/json", ".xlsx"]
        }
      ]
    }
  }
}
